{
    "name": "packettide/bree",
    "description": "A form layer for Eloquent which supports a variety of field packages",
    "keywords": [
        "laravel",
        "eloquent",
        "orm",
        "form",
        "fieldtypes"
    ],
    "authors": [
        {
            "name": "Bryan Nielsen",
            "email": "bryan@packettide.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "4.0.x",
        "illuminate/database": "4.0.x",
        "symfony/http-foundation": "2.3.*",
        "packettide/bree-fs-advanced": "dev-master"
    },
    "require-dev": {
        "mockery/mockery": "0.7.2",
        "phpunit/phpunit": "3.7.*"
    },
    "autoload": {
        "classmap": [
            "src/migrations"
        ],
        "psr-0": {
            "Packettide\\Bree": "src/"
        }
    },
    "minimum-stability": "dev"
}